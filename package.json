{
  "name": "Only hands",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "homepage": "https://mahhis.github.io/only-hands-landing",
  "scripts": {
    "start": "vite",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "deploy": "gh-pages -d dist",
    "predeploy": "yarn build",
    "pretty": "prettier --check src",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write \"src/classnames/tailwind.ts\"",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "preview": "yarn build && yarn vite preview"
  },
  "dependencies": {
    "countries-list": "^3.0.6",
    "dotenv": "^16.4.1",
    "envalid": "^8.0.0",
    "jotai": "^2.6.1",
    "preact": "^10.22.0",
    "preact-modal": "^1.0.13",
    "preact-router": "^4.1.2",
    "wouter": "^3.3.5",
    "wouter-preact": "^3.3.5"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/gh-pages": "^6",
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "daisyui": "^4.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3"
  },
  "packageManager": "yarn@4.0.2"
}
